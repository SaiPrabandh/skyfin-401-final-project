    <script>
        function expandProfile(profileId) {
            document.getElementById('full-' + profileId).style.display = 'block';
        }

        function closeProfile(profileId) {
            document.getElementById('full-' + profileId).style.display = 'none';
        }
            function searchProfiles() {
                const query = document.getElementById("searchInput").value.toLowerCase();
                const profiles = document.getElementsByClassName("freelancer-box");
                for (let i = 0; i < profiles.length; i++) {
                    const name = profiles[i].getElementsByTagName("h3")[0].innerText.toLowerCase();
                    if (name.includes(query)) {
                        profiles[i].style.display = "block"; 
                    } else {
                        profiles[i].style.display = "none";  
                    }
                }
            }
    </script>

